# http://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = tab
insert_final_newline = true
max_line_length = 80
trim_trailing_whitespace = true

[*.java]
continuation_indent_size = 8

[*.xml]
continuation_indent_size = 4
indent_size = 2

[COMMIT_EDITMSG]
max_line_length = 72
trim_trailing_whitespace = false
